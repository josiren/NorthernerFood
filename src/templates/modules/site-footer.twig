<footer class="site-footer l-footer">
    <div class="footer-top">
        <div class="footer-top__logo">
            {{ inc.inlineSvg('logo', '../static/img/footer/logo.svg') }}
        </div>
        <nav class="footer-top__navigation">
            <div class="navigation__text">О компании</div>
            <div class="navigation__text">Контакты</div>
            <div class="navigation__text">Вакансии</div>
            <div class="navigation__text">Статьи</div>
            <div class="navigation__text">Политика обработки персональных данных</div>
        </nav>
        <div class="footer-top__social">
            <div class="social__item">{{ inc.inlineSvg('instagram', '../static/img/footer/social/instagram.svg') }}</div>
            <div class="social__item">{{ inc.inlineSvg('vkontakte', '../static/img/footer/social/vkontakte.svg') }}</div>
            <div class="social__item">{{ inc.inlineSvg('facebook', '../static/img/footer/social/facebook.svg') }}</div>
            <div class="social__item">{{ inc.inlineSvg('ok', '../static/img/footer/social/ok.svg') }}</div>
        </div>
        <div class="footer-top__social-phone">
            <div class="social-phone__img">
                {{ inc.inlineSvg('phone', '../static/img/footer/social/phone.svg') }}
            </div>
            <p class="social-phone__text">8 800 777 33 33</p>
        </div>
    </div>
    <div class="footer-bottom">
        <p class="footer-bottom__text">Дизайн</p>
        <div class="footer-bottom__logo">
            {{ inc.inlineSvg('created-by', '../static/img/footer/created-by-logo.svg') }}
        </div>
    </div>
</footer>
